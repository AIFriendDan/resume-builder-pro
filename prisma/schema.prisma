generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
    
}

model Resume {
  id          String   @id @default(uuid())
  title       String
  content     Json     // This stores your entire resume object (experience, education, etc.)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}